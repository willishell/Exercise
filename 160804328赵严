#练习1
touch message.txt
mv message.txt ./Web
cp ./Web/message.txt ~/
unlink ~/message.txt && unlink ./Web/message.txt

#练习2
mkdir ~/workbench
touch ~/workbench/readme.txt
echo -e "1\n2\n3">~/workbench/readme.txt
cat ~/workbench/readme.txt
ls ~/>~/workbench/list.txt
wc -m ~/workbench/list.txt

